{
"super_brake_F": {
    "information":{
        "authors":"BeamNG",
        "name":"Front Disc Brakes",
        "value":500,
    }
    "slotType" : "super_brake_F",
    "slots": [
        ["type", "default", "description"]
        ["brakepad_F","brakepad_F_sport", "Front Brake Pads"{"coreSlot":true}],
    ],
    "flexbodies": [
        ["mesh", "[group]:", "nonFlexMaterials"],
        //brake discs
        ["super_brakedisc_FR", ["wheel_FR","wheelhub_FR"], ["super"], {"scale":{"x":0.98, "y":1, "z":1}}],
        ["super_brakedisc_FL", ["wheel_FL","wheelhub_FL"], ["super"], {"scale":{"x":0.98, "y":1, "z":1}}],
    ],
    "pressureWheels": [
        ["name","hubGroup","group","node1:","node2:","nodeS","nodeArm:","wheelDir"],
        {"brakeTorque":1600},
        {"brakeInputSplit":1},
        {"brakeSplitCoef":1},
        {"parkingTorque":0},
        {"brakeSpring":125},
        
        //brake thermals
        {"enableBrakeThermals":true},
        {"brakeDiameter":0.295},
        {"brakeMass":9.5},
        {"brakeType":"vented-disc"},
        {"rotorMaterial":"steel"},
        {"brakeVentingCoef":1.0},
    ],
}
