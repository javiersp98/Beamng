"coupe_taillight_R_dark": {
//"begin"
	"information":{
		"authors":"BeamNG (Edited by Jesper, AKA D-TROXX)",
		"name":"Dark Right Taillight",
		"value":60,
	}
    "slotType" : "coupe_taillight_R",
