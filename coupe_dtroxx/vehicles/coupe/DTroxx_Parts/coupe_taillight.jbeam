			"lightRange":20
			"lightColor":{"r":255, "g":50, "b":0, "a":255}
			"lightAttenuation":{"x":0, "y":1, "z":1}
			"lightCastShadows":false
            "flareName":"vehicleBrakeLightFlare"
            "flareScale":0.4   
		}
		//corner
		["signal_R" , 		"SPOTLIGHT"        , "q11r", "q12r", "q6r", {"x":0, "y":0, "z":-15} , {"x":0, "y":0, "z":0}    , {"x":0, "y":0, "z":0}, 0, 0, 0, 1, {"baseTranslation":{"x":0.6, "y":0.7, "z":-0.01},"deformGroup":"coupe_taillight_R_break"}],
		["lowhighbeam" , 		"SPOTLIGHT"        , "q11r", "q12r", "q6r", {"x":0, "y":0, "z":-15} , {"x":0, "y":0, "z":0}    , {"x":0, "y":0, "z":0}, 0, 0, 0, 1, {"baseTranslation":{"x":0.6, "y":0.7, "z":-0.01},"deformGroup":"coupe_taillight_R_break"}],
	 ],
	 "glowMap":{			
			//dark
			"coupe_taillight_R_dark":{"simpleFunction":{"brake":0.49,"lowhighbeam":0.49,"signal_R":0.49}, "off":"coupe_lights_dark", "on":"coupe_lights_on_dark", "on_intense":"coupe_lights_on_intense_dark"},
	}
}
