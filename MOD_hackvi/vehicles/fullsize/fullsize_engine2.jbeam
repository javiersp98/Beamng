        "lowShiftUpRPM":2000,
    },
    "flexbodies": [
        ["mesh", "[group]:", "nonFlexMaterials"],
        ["fullsize_transmission", ["fullsize_engine","fullsize_transmission"]],
    ],
    "nodes": [
